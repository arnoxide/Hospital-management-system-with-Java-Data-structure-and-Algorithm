 CheckupList cul = new CheckupList();
        
        Checkup c1 = new Checkup(d1,p1,3,"A","55");
        
        Checkup c2 = new Checkup(d1,p1,2,"B","55");
         
         Checkup c3 = new Checkup(d1,p1,3,"C","55");
          
         Checkup c4 = new Checkup(d1,p1,1,"D","55"); 
          
         Checkup c5 = new Checkup(d1,p1,2,"E","55");
         
         Checkup c6 = new Checkup(d1,p1,3,"F","55");
         
         Checkup c7 = new Checkup(d1,p1,3,"G","55");
         
         cul.Enqueue(c1);
         cul.Enqueue(c2);
         cul.Enqueue(c3);
         cul.Enqueue(c4);
         cul.Enqueue(c5);
         cul.Enqueue(c6);
         cul.Enqueue(c7);
         
         cul.Print();
