        Doctor d1= new Doctor("25","Arnold","000000","Eyes",600);
        Doctor d2= new Doctor("26","Orifha","11111","Gynacaelogist",700);
        Doctor d3= new Doctor("27","Fhulu","22222","physio",800);
        
        DoctorsList dl = new DoctorsList();
